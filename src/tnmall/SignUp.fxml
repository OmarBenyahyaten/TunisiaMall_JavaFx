<AnchorPane id="signup" prefHeight="551.0" prefWidth="517.0" style="-fx-opacity: ;" stylesheets="@../style/MainStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tnmall.SignUpController">
   <children>
      <Label layoutX="204.0" layoutY="35.0" text="Signup" textFill="WHITE">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <GridPane layoutX="74.0" layoutY="120.0" prefHeight="329.0" prefWidth="383.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="tfUser" onKeyReleased="#onKeyReleaseLoginOnAction" prefHeight="46.0" prefWidth="412.0" promptText="username" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <TextField fx:id="tfMail" onKeyReleased="#onKeyEmailLoginOnAction" prefHeight="46.0" prefWidth="412.0" promptText="Email" GridPane.rowIndex="3" />
            <PasswordField fx:id="tfPassword" onKeyReleased="#onKeyPassWordLoginOnAction" prefHeight="46.0" prefWidth="412.0" promptText="Password" GridPane.rowIndex="1" />
            <PasswordField fx:id="tfPasswordConfirm" onKeyReleased="#onKeyPassWordConfirmLoginOnAction" prefHeight="46.0" prefWidth="412.0" promptText="Confirm Password" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button fx:id="btnSignup" layoutX="201.0" layoutY="469.0" mnemonicParsing="false" onAction="#onSignUpOnAction" prefHeight="36.0" prefWidth="129.0" text="SignUp" />
      <Button fx:id="btnClose" layoutX="474.0" mnemonicParsing="false" onAction="#btnCloseOnAction" prefHeight="36.0" prefWidth="43.0" text="X" />
      <Label fx:id="lbPassword" layoutX="78.0" layoutY="276.0" prefHeight="17.0" prefWidth="383.0" />
      <Label fx:id="lbPasswordConfirm" layoutX="77.0" layoutY="360.0" prefHeight="17.0" prefWidth="383.0" />
      <Label fx:id="lbEmail" layoutX="79.0" layoutY="441.0" prefHeight="17.0" prefWidth="71.0" />
      <Label fx:id="lbLogin" layoutX="74.0" layoutY="191.0" prefHeight="30.0" prefWidth="383.0" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
